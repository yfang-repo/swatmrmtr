#================================================
# Description: DFN with 2 intersecting fractures
# Test problem for richards equation
# Satish Karra, LANL
# Email: satkarra@lanl.gov
# April 08, 2013
#================================================

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
  /
END

SUBSURFACE

DFN

#========================== regression ========================================
REGRESSION
  CELLS
    1
    3
    5
    7
    9
    11
    13
    15
  /
END

#=========================== flow mode ========================================

#=========================== discretization ===================================
GRID
  TYPE unstructured_explicit dfn.uge
END

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
END

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0
  TORTUOSITY 0.5d0
  CHARACTERISTIC_CURVES default
  PERMEABILITY
    PERM_ISO 1.d-12
  /
END

#=========================== characteristic curves ============================
CHARACTERISTIC_CURVES default
  SATURATION_FUNCTION BROOKS_COREY
    ALPHA 1.d-4
    LAMBDA 0.3d0
    LIQUID_RESIDUAL_SATURATION 0.1d0
    SMOOTH
  /
  PERMEABILITY_FUNCTION MUALEM_BC_LIQ
    LAMBDA 0.3d0
    LIQUID_RESIDUAL_SATURATION 0.1d0
  /
END

#=========================== output options ===================================
OUTPUT
  FORMAT TECPLOT BLOCK
  HDF5_WRITE_GROUP_SIZE 1
  PRINT_PRIMAL_GRID
END

#=========================== times ============================================
TIME
  FINAL_TIME 100.d0 s
  MAXIMUM_TIMESTEP_SIZE 10.d0 d
END

REFERENCE_PRESSURE 101325.

#=========================== regions ==========================================
REGION All
  COORDINATES
    -1.d20 -1.d20 -1.d20
    1.d20 1.d20 1.d20
  /
END


REGION Front
  FILE dfn_front.ex
END

#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
     PRESSURE hydrostatic
  /
  DATUM 0.d0 0.d0 1.d0
  PRESSURE 101325.d0
END


FLOW_CONDITION front
  TYPE
     PRESSURE dirichlet
  /
  PRESSURE 2.d5
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  REGION All
END


BOUNDARY_CONDITION Front
  FLOW_CONDITION front
  REGION Front
END


#=========================== stratigraphy couplers ============================
STRATA
  REGION All
  MATERIAL soil1
END
END_SUBSURFACE
