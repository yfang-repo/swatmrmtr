#Description: 0D test problem for testing fuel matrix degradation model

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
    SUBSURFACE_TRANSPORT transport
      GLOBAL_IMPLICIT
    /
    WASTE_FORM fmdm_test
      TYPE GENERAL
    /
  /
END

SUBSURFACE
#=========================== regression =======================================
REGRESSION
  CELLS
    1
  /
END

#=========================== chemistry ========================================
CHEMISTRY
  PRIMARY_SPECIES
    O2(aq)
    HCO3-
    H2(aq)
    Fe++
    Tracer
  /
  OUTPUT
    TOTAL
    Tracer
  /
END

#=========================== solver options ===================================
TIMESTEPPER FLOW
  TS_ACCELERATION 8
END

NEWTON_SOLVER FLOW
END

LINEAR_SOLVER FLOW
#  KSP_TYPE PREONLY
#  PC_TYPE LU
  LU_ZERO_PIVOT_TOL 1.d-40
END

NEWTON_SOLVER TRANSPORT
END

LINEAR_SOLVER TRANSPORT
#  KSP_TYPE PREONLY
#  PC_TYPE LU
END

#=========================== discretization ===================================
GRID
  TYPE structured
  NXYZ 1 1 1
  BOUNDS
    0.d0 0.d0 0.d0
    1.d0 1.d0 1.d0
  END
END

#=========================== fluid properties =================================
FLUID_PROPERTY 
  DIFFUSION_COEFFICIENT 1.d-9
END

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    PERM_X 1.d-12
    PERM_Y 1.d-12
    PERM_Z 1.d-12
  /
END

#=========================== characteristic curves ============================
CHARACTERISTIC_CURVES sf1
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.286
    ALPHA  1.9401d-4
    LIQUID_RESIDUAL_SATURATION 0.115
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.286
    LIQUID_RESIDUAL_SATURATION 0.115
  /
END

#=========================== output options ===================================
OUTPUT
  VARIABLES
  /
END

#=========================== times ============================================
TIME
  FINAL_TIME 500.d0 yr
  INITIAL_TIMESTEP_SIZE 1.d-6 d
  MAXIMUM_TIMESTEP_SIZE 2.0 yr
END

#=========================== regions ==========================================
REGION all
  COORDINATES
    -1.d20 -1.d20 -1.d20
    1.d20 1.d20 1.d20
  /
END

#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
    PRESSURE dirichlet
  /
  PRESSURE 201325 ! Pa
END

#=========================== transport conditions =============================
TRANSPORT_CONDITION groundwater
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    0.d0 groundwater
  /
END

#=========================== transport constraints ============================
CONSTRAINT groundwater
  CONCENTRATIONS
    O2(aq)   1.d-3           F
    HCO3-    1.d-3           F
    H2(aq)   1.d-3           F
    Fe++     1.d-3           F
    Tracer   1.e-7            F
  /
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION groundwater
  REGION all
END

#=========================== stratigraphy couplers ============================
STRATA
  MATERIAL soil1
  REGION all
END

#=========================== checkpoint / restart =============================
#CHECKPOINT 10

END_SUBSURFACE


WASTE_FORM_GENERAL

  #BYPASS_WARNING_MESSAGE

  WASTE_FORM
    COORDINATE 0.5d0 0.5d0 0.5d0
    VOLUME 1.d0 m^3
    MECHANISM_NAME fmdm01
    CANISTER_VITALITY_RATE 0.0003 1/day
  /

  MECHANISM FMDM # (dissolution rate determined by FMDM)
    NAME fmdm01
    MATRIX_DENSITY 19000. kg/m^3
    SPECIFIC_SURFACE_AREA 0.001 m^2/g
    BURNUP 1.0d-2 kg/m^2-yr  # (because FMDM is not linked)
    SPECIES
     #name,   MW[g/mol],dcy[1/s], initMF, inst_rel_frac,daughter
     Tracer   233.04d0  1.38d-13  9.70d-9   0.0d0
    /
    CANISTER_DEGRADATION_MODEL
      CANISTER_MATERIAL_CONSTANT 1500.0
    /
  /

  PRINT_MASS_BALANCE

END_WASTE_FORM_GENERAL
